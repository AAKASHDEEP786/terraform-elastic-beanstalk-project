aws_region   = "ap-south-1"
project_name = "demo-project"

vpc_cidr = "10.0.0.0/16"

public_subnet_cidrs = [
  "10.0.1.0/24",
  "10.0.2.0/24"
]

private_subnet_cidrs = [
  "10.0.3.0/24",
  "10.0.4.0/24"
]

public_subnet_azs  = ["ap-south-1a", "ap-south-1b"]
private_subnet_azs = ["ap-south-1a", "ap-south-1b"]

eb_platform      = "64bit Amazon Linux 2023 v6.6.8 running Node.js 20"
eb_instance_type = "t3.micro"

db_engine         = "postgres"
db_engine_version = "17.6"
db_instance_class = "db.t3.micro"
db_name           = "appdb"
db_username       = "example_username"
db_password       = "example_password_123!"

enable_cloudwatch_logs = true

tags = {
  Environment = "dev"
  Project     = "demo-project"
  ManagedBy   = "Terraform"
}
